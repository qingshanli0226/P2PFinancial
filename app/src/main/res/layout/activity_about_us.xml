<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.SecondaryPage.AboutUsActivity"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="" />
    </LinearLayout>
    <TextView
        android:id="@+id/homeActivityTittleBarId"
        android:textColor="@color/homeActivityTittleBarTextColor"
        android:background="@color/homeActivityTittleBarBackground"
        android:textSize="@dimen/homeActivityTittleBarTextSize"
        android:gravity="center"
        android:text="@string/radioButtonHomePage"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
</TextView>