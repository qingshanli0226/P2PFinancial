<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/MainActivityWidthHeight"
    tools:context=".mvp.view.activity.MainActivity"
    android:orientation="vertical">
    <jni.example.p2pinvest.view.TopBar
        android:id="@+id/top_bar"
        style="@style/MainActivityWidthHeight"/>
    <FrameLayout
        android:id="@+id/frame"
        android:layout_below="@id/top_bar"
        style="@style/MainActivityWidthHeight"
        android:layout_above="@id/main_myview"/>
    <jni.example.p2pinvest.view.BottomBar
        android:layout_alignParentBottom="true"
        android:id="@+id/main_myview"
        style="@style/MainActivityWidthHeight"/>
</RelativeLayout>